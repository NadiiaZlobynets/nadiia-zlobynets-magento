<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <move element="nadiiaz.cms.container" destination="page.wrapper" before="-"/>
        <referenceContainer name="content">
        <container name="nadiiaz.cms.container" htmlTag="div" htmlClass="nadiiaz-cms">
            <block class="Magento\Cms\Block\BlockByIdentifier" name="nadiiaz.cms.block">
                <arguments>
                    <argument name="identifier" xsi:type="string">nadiiaz_cms_block</argument>
                </arguments>
            </block>
        </container>
        </referenceContainer>
        <referenceContainer name="nadiiaz.cms.img.container">
            <block class="Magento\Cms\Block\BlockByIdentifier" name="nadiiaz.cms.img.block">
                <arguments>
                    <argument name="identifier" xsi:type="string">nadiiaz_cms_img_block</argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
